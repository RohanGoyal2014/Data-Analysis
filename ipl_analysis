    "player = dict()\n",
    "for index,row in match_df.iterrows():\n",
    "    mid = row['id']\n",
    "    mom = row['player_of_match']\n",
    "    if mid in mapping:\n",
    "        if mom in mapping[mid]:\n",
    "            if mom not in player:\n",
    "                player[mom]=0\n",
    "            player[mom]+=1"
   ]
