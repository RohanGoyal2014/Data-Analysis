   "outputs": [],
   "source": [
    "d = dict()\n",
    "for index, row in match_df.iterrows():\n",
    "    venue = row['venue']\n",
    "    if venue not in d:\n",
    "        d[venue]=0\n",
    "    if row['result'] == 'normal':\n",
    "        if row['winner'] == row['team1']:\n",
    "            d[venue]+=1\n",
    "        elif row['winner'] == row['team2']:\n",
    "            d[venue]-=1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in d:\n",
    "    if d[i]==0:\n",
    "        d[i]='any'\n",
    "    elif d[i]>0:\n",
    "        d[i]='bat'\n",
    "    else:\n",
    "        d[i]='field'"
   ]
