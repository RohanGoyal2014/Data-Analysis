    "#Print the name of the umpire who was present in maximum number of matches in each year\n",
    "umpire_data = dict()\n",
    "for index,row in match_df.iterrows():\n",
    "    year = row['season']\n",
    "    umpire1 = row['umpire1']\n",
    "    umpire2 = row['umpire2']\n",
    "    if year not in umpire_data:\n",
    "        umpire_data[year] = dict()\n",
    "    if umpire1 not in umpire_data[year]:\n",
    "        umpire_data[year][umpire1]=0\n",
    "    if umpire2 not in umpire_data[year]:\n",
    "        umpire_data[year][umpire2]=0\n",
    "    umpire_data[year][umpire1]+=1\n",
    "    umpire_data[year][umpire2]+1\n",
    "for year in umpire_data:\n",
    "    print(year,':',max(umpire_data[year],key=umpire_data[year].get))"
