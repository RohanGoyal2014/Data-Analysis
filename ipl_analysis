    "# Print team involved in maximum number of super overs\n",
    "super_over_teams = dict()\n",
    "for index, row in delivery_df.iterrows():\n",
    "    key1 = row['batting_team']\n",
    "    key2 = row['bowling_team']\n",
    "    if key1 not in super_over_teams:\n",
    "        super_over_teams[key1]=set()\n",
    "    if key2 not in super_over_teams:\n",
    "        super_over_teams[key2]=set()\n",
